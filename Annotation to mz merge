library(openxlsx)
library(dplyr)

data_df  <- read.xlsx("correctpath/file.xlsx")
annot_df <- read.xlsx("correctpath/file.xlsx.xlsx")

data_df$mz  <- as.numeric(data_df$mz)
annot_df$mz <- as.numeric(annot_df$mz)

merged_df <- full_join(data_df, annot_df, by = "mz")

write.xlsx(
  merged_df,
  "correctpath/result_file.xlsx",
  rowNames = FALSE
)
